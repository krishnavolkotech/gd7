{
    "name": "drupal/drupal",
    "description": "Drupal is an open source content management platform powering millions of websites and applications.",
    "type": "project",
    "license": "GPL-2.0+",
    "require": {
        "composer/installers": "^1.0.24",
        "wikimedia/composer-merge-plugin": "^1.4",
        "drupal/config_direct_save": "^1.0",
        "drupal/search_api": "^1.3",
        "drupal/view_unpublished": "^1.0@alpha",
        "zendframework/zend-stdlib": "^3.0",
        "drupal/migrate_manifest": "^1.4",
        "drupal/entity_print": "2.x",
        "dompdf/dompdf": "^0.8.0",
        "drupal/swiftmailer": "^1.0@beta",
        "symfony/dom-crawler": "2.8",
        "drupal/subpathauto": "^1.0@beta",
        "drupal/login_emailusername": "^1.1",
        "drupal/flood_unblock": "^1.3",
        "drupal/simple_block": "^1.0@beta",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/queue_mail": "^1.0@alpha",
        "drupal/block_visibility_groups": "^1.1",
        "drupal/menu_export": "^1.0",
        "drupal/views_merge_rows": "^2.1",
        "drupal/back_to_top": "^1.0@beta",
        "drupal/anchor_link": "^1.5",
        "drupal/admin_toolbar": "^1.19",
        "drupal/block_class": "^1.0@alpha",
        "drupal/block_upload": "1.x-dev",
        "drupal/ctools": "^3.0",
        "drupal/draggableviews": "^1.0",
        "drupal/field_group": "^1.0@RC",
        "drupal/field_permissions": "^1.0@beta",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/l10n_client": "^1.0@alpha",
        "drupal/mailsystem": "^4.1",
        "drupal/masquerade": "^2.0@beta",
        "drupal/migrate_plus": "^2.0",
        "drupal/pathauto": "^1.0",
        "doctrine/inflector": "^1.1",
        "doctrine/common": "^2.6",
        "drupal/group": "^1.0@RC",
        "drupal/bootstrap": "3.11",
        "solarium/solarium": "^3.8",
        "drupal/search_api_solr": "^1.0",
        "drupal/search_api_solr_multilingual": "^1.0@beta",
        "drupal/allowed_formats": "^1.1",
        "drupal/computed_field": "^2.0@alpha",
        "drupal/imce": "^1.6",
        "drupal/persistent_login": "^1.0@alpha",
        "drupal/filefield_sources": "1.x-dev",
        "drupal/prlp": "1.3.0",
        "drupal/piwik": "1.2.0",
        "drupal/reroute_email": "1.0.0",
        "drupal/queue_ui": "2.x@dev",
        "drupal/devel": "1.2",
        "drupal/faq": "1.x@dev",
        "drupal/faqfield": "^1.1",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/serial": "1.x-dev",
        "drupal/entity_reference_exposed_filters": "^1.0@alpha",
        "drupal/revision_log": "1.0-alpha1",
        "drush/drush": "^9.5",
        "drupal/console": "^1.8"
    },
    "replace": {
        "drupal/core": "^8.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8"
    },
    "extra": {
        "_readme": [
            "By default Drupal loads the autoloader from ./vendor/autoload.php.",
            "To change the autoloader you can edit ./autoload.php."
        ],
        "merge-plugin": {
            "include": [
                "core/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "merge-extra": false
        },
        "installer-paths": {
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/{$name}": ["type:drupal-theme"]
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Drupal\\Core\\Composer\\Composer::preAutoloadDump",
        "post-autoload-dump": "Drupal\\Core\\Composer\\Composer::ensureHtaccess",
        "post-package-install": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "post-package-update": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    }
}
